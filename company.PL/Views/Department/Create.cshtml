    </div>
     <div class="form-group">
        <label class="col-form-label" asp-for="Name"></label>
        <input  class="form-control" asp-for="Name"/>
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="DataOfcreation"></label>
        <input class="form-control" asp-for="DataOfcreation" />
        <span asp-validation-for="DataOfcreation" class="text-danger"></span>
    </div>
    <input  type="submit" value="Sumbit" class="btn btn-primary"/>||
    <a asp-action="Index" class="btn btn-light">Back to List</a>
</form>
