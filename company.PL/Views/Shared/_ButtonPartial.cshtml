@model string 
<td>
    <a       asp-action="Details"  asp-route-id="@Model">
        <i class="fas fa-eye" style="color:blue;"></i>
    </a>
</td>
<td>
    <a asp-action="Edit"  asp-route-id="@Model">
        <i class="fa-solid fa-pen-to-square" style="color:Gold;"></i>
    </a>
</td>
<td>
     <a asp-action="Delete"  asp-route-id="@Model"> 
        <i  class="fas fa-trash" style="color:red;"></i>  
    </a>
</td>
